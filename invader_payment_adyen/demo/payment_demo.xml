<?xml version="1.0" encoding="UTF-8"?>
<odoo noupdate="1">

    <record id="bank_journal_adyen" model="account.journal">
        <field name="name">Adyen</field>
        <field name="type">bank</field>
        <field name="code">BNKADY</field>
    </record>

    <record id="payment_mode_adyen" model="account.payment.mode">
        <field name="name">Adyen</field>
        <field name="bank_account_link">fixed</field>
        <field name="fixed_journal_id" ref="bank_journal_adyen"/>
        <field name="payment_method_id" ref="payment.account_payment_method_electronic_in"/>
        <field name="payment_acquirer_id" ref="payment.payment_acquirer_adyen"/>
    </record>

</odoo>
